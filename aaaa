class Car:
    def __init__(self, color: str = 'Silver', power: int = 70, fuel: float = 50, rashod: float = 25, fuel_shas: float = 30, rashod_km: float = 0.25):
        self.color = color
        self.power = power
        self.fuel = fuel
        self.rashod = rashod
        self.fuel_shas = fuel_shas
        self.rashod_km = rashod_km
    def get_color(self):
        return self.color
    def get_power(self):
        return self.power
    def get_fuel(self):
        return self.fuel
    def get_rashod(self):
        return self.rashod
    def get_rashod_km(self):
        return self.rashod_km
    def set_color(self, color):
        self.color = color
        return self.color
    def set_fuel (self, fuel):
        self.fuel = fuel
        return self.fuel
    def get_fuel_shas (self):
        return self.fuel_shas
    def get_benzin (self):
        return self.benz_shas
    def set_fuel_shas (self, fuel_shas):
        self.fuel_shas = fuel_shas
        return self.fuel_shas
    def get_zapravka (self):
        return self.fuel - self.fuel_shas
    def zapravka (self, benzin):
        if self.fuel >= self.fuel_shas:
            if benzin > c.get_zapravka():
                print ('Столько не залить')
            else:
                 self.fuel_shas += benzin
                 return self.fuel_shas
        else:
            print ('Тебе хватит')
    def probeg(self):
        return self.fuel_shas / self.rashod_km

c = Car(color = 'Black', power = 70, fuel = 50, rashod = 25, fuel_shas = 30, rashod_km = 0.25)
print ('Цвет:', c.get_color())
print ('Мощность:', c.get_power())
print ('Вместимость бакака:', c.get_fuel())
print ('Расход:', c.get_rashod())
print ('Можно проехать:', c.probeg())

print ()

c.set_color('Black')
c.set_fuel(50)
print (f'Перекрашена в: {c.get_color()} цвет' )
print (f'Максимальная вместимость бака: {c.get_fuel()} литров')
print (f'В баке щас: {c.get_fuel_shas()} литров')
print (f'Можно залить на: {c.get_zapravka()} литров')
c.zapravka(10)
print (f'Можно проехать: {c.probeg()}')
